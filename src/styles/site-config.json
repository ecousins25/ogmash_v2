{
  "colors": {
    "primary": "#FF6B6B",
    "secondary": "#4ECDC4",
    "accent": "#45B7D1",
    "background": {
      "main": "#87CEEB",
      "secondary": "#A7D8F3"
    },
    "text": {
      "primary": "#000000",
      "secondary": "#333333"
    }
  },
  "spacing": {
